<h1>All companies</h1>
<%= link_to "New Company", new_company_path %>
<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Country</th>
  </tr>
  </thead>
  <tbody>
  <%@companies.each do |company| %>
    <tr>
      <td><%= company.name%></td>
      <td><%= company.country%></td>
      <td>
        <%= link_to "show", company_path(company)%>
        <%= link_to "edit", edit_company_path(company)%>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>